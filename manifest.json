{
	"name":"tag",
	"version":"1.0",
	"manifest_version":2,
	"permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "<all_urls>"
],
  "content_scripts":
  [
  {
    "matches": [
		"<all_urls>"
    ],
    "js": ["content.js"]
  }
  ],
	"background":{
		"matches":["<all_urls>"],
		"scripts":["background.js"]
	}
}